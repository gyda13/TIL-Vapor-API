<style media="screen">
 

body{
align-items: center;
justify-content: center;
min-height: 150vh;
background-color: #184e77;
}
 
.background{
    width: 430px;
    height: 520px;
    position: absolute;
    transform: translate(-50%,-50%);
    left: 50%;
    top: 50%;
}
.background .shape{
    height: 280px;
    width: 280px;
    position: absolute;
    border-radius: 50%;
}
.shape:first-child{
    background: linear-gradient(
        #52b69a,
        #76c893
    );
    left: -400px;
    bottom: -100px;
}


.wrapper,
.buttons button{
background-color: rgba(255,255,255,0.13);
border-radius: 10px;
backdrop-filter: blur(10px);
border: 2px solid rgba(255,255,255,0.1);
box-shadow: 0 0 20px rgba(8,7,16,0.6);
}

.wrapper{
position: relative;
width: 1000px;
height: 600px;
padding: 30px;
border-radius: 10px;
display: flex;
align-items: center;
justify-content: center;
flex-direction: column;
}

.wrapper .name{
font-size: 23px;
font-weight: 500;
color: white;
margin: 10px 0 5px 0;
}
.wrapper .about{
color: #d0d1d7;
font-weight: 400;
font-size: 16px;
}
h1{
color: white;
}
h2{
color: white;
}
h3{
color: white;
}
th{
color: white;
}
td{
color: white;
}
.table{
width: 80%;
}
a {
color: #52b69a;
}
u {
color: #b5e48c;
}
 </style>

</head>

